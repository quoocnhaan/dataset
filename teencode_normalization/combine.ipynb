    "import pandas as pd\n",
    "\n",
    "# Load CSV files\n",
    "df1 = pd.read_csv(\"instruction_imbalance.csv\")\n",
    "df2 = pd.read_csv(\"instruction_balance.csv\")\n",
    "\n",
    "# Concatenate vertically (stack rows)\n",
    "df = pd.concat([df1, df2], ignore_index=True)\n",
    "\n",
    "# Save to a new CSV\n",
    "df.to_csv(\"combined.csv\", index=False)\n"
   ]
